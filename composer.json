{
    "name": "kontsevoye/collabatube",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware",
        "youtube",
        "collaboration"
    ],
    "description": "Collabatube.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.5",
        "ext-json": "*",
        "hyperf/cache": "~2.0.0",
        "hyperf/command": "~2.0.0",
        "hyperf/config": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/http-server": "~2.0.0",
        "hyperf/logger": "~2.0.0",
        "hyperf/memory": "~2.0.0",
        "hyperf/process": "~2.0.0",
        "hyperf/socketio-server": "^2.0",
        "hyperf/db-connection": "^2.0",
        "hyperf-ext/auth": "^2.0",
        "hyperf-ext/jwt": "^2.0",
        "hyperf/guzzle": "^2.0",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.1",
        "symfony/validator": "^5.1",
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "symfony/cache": "^5.1"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "hyperf/devtool": "~2.0.0",
        "hyperf/testing": "~2.0.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs": "php-cs-fixer fix $1 --dry-run",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 5 -c phpstan.neon ./app ./config",
        "start": "php ./bin/hyperf.php start"
    }
}
